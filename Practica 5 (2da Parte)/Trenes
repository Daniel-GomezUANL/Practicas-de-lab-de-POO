
package Practica5;

    public class Trenes extends Transporte{
    private String Marca;
    private String Modelo;
    private int Precio;
    private String Color;

    public String getMarca() {
        return Marca;
    }
    public void setMarca(String Marca) {
        this.Marca = Marca;
    }
    public String getModelo() {
        return Modelo;
    }
    public void setModelo(String Modelo) {
        this.Modelo = Modelo;
    }
    public int getPrecio() {
        return Precio;
    }
    public void setPrecio(int Precio) {
        this.Precio = Precio;
    }
    public String getColor() {
        return Color;
    }
    public void setColor(String Color) {
        this.Color = Color;
    }
    
    @Override
    public void encender(){
        System.out.println("Encender");
    }
    public void Derecha(){
        System.out.println("Vuelta a la Derecha");
    }
    public void Izquierda(){
        System.out.println("Vuelta a la Izquierda");
    }
    
    @Override
    public void apagar(){
        System.out.println("Apagado");
    }
    
    @Override
    public void acelerar(){
        System.out.println("Acelerar");
    }
    
    public Trenes( String Marca, String Modelo, String Color,int Precio){
    this.Marca = Marca;
    this.Modelo = Modelo;
    this.Color = Color;
    this.Precio = Precio;
    }
    @Override
    public String toString(){
        return "Trenes:" + " Marca:" + Marca + " Modelo:" + Modelo + " Color:"+ Color + " Precio:" + Precio + '}';
    }

    
}
