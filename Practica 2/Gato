
package Animal;

public class Gato extends Animal{
    //// PRIVADO ////
    private String Raza;
    public void setRaza(String raza){
        Raza = raza;
    }
    public String getRaza(){
        return Raza;
    }
    //// PUBLICO ////
    public void comer(){
        System.out.println("Estoy comiendo");
    }
    public void maullido(){
        System.out.println("Estoy maullando");
    }
    public void brincar(){
        System.out.println("Estoy brincando");
    }
}
